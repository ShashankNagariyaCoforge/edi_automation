{
  "record_number": "4800",
  "record_name": "Ship-To Address (Line Level)",
  "record_purpose": "Holds line-level Ship-To party address and organizational information when it differs from the header Ship-To",
  "record_classification": {
    "record_type_identifier": {
      "value": "AD",
      "source": "oracle_standard",
      "description": "Identifies this record as an address record"
    },
    "record_layout_qualifier": {
      "value": "ST1",
      "source": "oracle_standard",
      "description": "Ship-To address layout (line-level)"
    }
  },
  "fields": {
    "TP_Translator_Code": {
      "description": "Trading partner identifier used by the EDI translator",
      "semantic_role": "trading_partner_identification",
      "value_source": "x12",
      "x12_mapping": {
        "segment": "GS",
        "element": "02",
        "name": "Application Sender's Code"
      },
      "value_type": "inferred"
    },

    "Header_Identifier_Location_Identifier": {
      "description": "Unique identifier of the purchase order header",
      "semantic_role": "transaction_identity",
      "value_source": "x12",
      "x12_mapping": {
        "segment": "BEG",
        "element": "03",
        "name": "Purchase Order Number"
      },
      "value_type": "inferred"
    },

    "Detail_Line_Identifier": {
      "description": "Reference to the purchase order line this Ship-To applies to",
      "semantic_role": "line_reference",
      "value_source": "x12",
      "x12_mapping": {
        "segment": "PO1",
        "element": "01",
        "name": "Assigned Identification"
      },
      "value_type": "inferred"
    },

    "Sub_Detail_Line_Identifier": {
      "description": "Sub-line identifier for split shipments or components",
      "semantic_role": "sub_line_reference",
      "value_source": "constant",
      "value_type": "constant",
      "fixed_value": ""
    },

    "Record_Number": {
      "description": "Oracle record number",
      "semantic_role": "record_identity",
      "value_source": "constant",
      "value_type": "constant",
      "fixed_value": "4800"
    },

    "Ship_To_Address_Code": {
      "description": "Ship-To address code used internally by Oracle at line level",
      "semantic_role": "erp_address_identifier",
      "value_source": "erp_lookup",
      "value_type": "lookup",
      "notes": "Resolved via Oracle customer ship-to site setup"
    },

    "Ship_To_EDI_Location_Code": {
      "description": "EDI location identifier for the Ship-To party",
